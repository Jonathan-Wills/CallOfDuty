@using CallOfDuty.Models
@model Root

<h1 style="color:white">@Model.data.username's Stats</h1>
<link rel="stylesheet" href="~/css/Searchpage.css">
<table class="table">
    <tr>
        <th style="color:white; width:25%;">Wins</th>
        <th style="color:white; width:25%;">Kills</th>
        <th style="color:white; width:25%;">Deaths</th>
        <th style="color:white; width:25%;">K/D Ratio</th>
        <th style="color:white; width:25%;">Objective Time</th>
        <th style="color:white; width:25%;">Contracts</th>
        <th style="color:white; width:25%;">Revives</th>
        <th style="color:white; width:25%;">Downs</th>
        <th style="color:white; width:25%;">Score</th>
        <th style="color:white; width:25%;">Top 25</th>
        <th style="color:white; width:25%;">Top 10</th>
        <th style="color:white; width:25%;">Top 5</th>
        <th style="color:white; width:25%;">Score Per Minute</th>
        <th style="color:white; width:25%;">Time Played</th>
        <th style="color:white; width:25%;">Games Played</th>
    </tr>
    <tr>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.wins</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.kills</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.deaths</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.kdRatio</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.objTime</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.contracts</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.revives</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.downs</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.score</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.topTwentyFive</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.topTen</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.topFive</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.scorePerMinute</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.timePlayed</td>
        <td style="color:white; width:25%;">@Model.data.lifetime.mode.br_all.properties.gamesPlayed</td>

    </tr>

</table>

<div style="display: block"></div>
